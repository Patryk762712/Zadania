public class Adress {
    private String adress;
    private String city;

    public Adress(String adress, String city){
        this.adress = adress;
        this.city = city;
    }

    public String getAdress() {
        return adress;
    }

    public void setAdress(String adress) {
        this.adress = adress;
    }

    public String getCity() {
        return city;
    }

    public void setCity(String city) {
        this.city = city;
    }
}

